{% extends 'base.html.twig' %}

{% block title %}Parcours index{% endblock %}

{% block body %}
    <h1>Parcours index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NumPoleEmploi</th>
                <th>Cpf</th>
                <th>DernierDiplome</th>
                <th>DernierEmploi</th>
                <th>Dernier_contrat</th>
                <th>Handicap</th>
                <th>Remarque</th>
                <th>Cv</th>
                <th>NomReferent</th>
                <th>MailReferent</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for parcour in parcours %}
            <tr>
                <td>{{ parcour.id }}</td>
                <td>{{ parcour.numPoleEmploi }}</td>
                <td>{{ parcour.cpf }}</td>
                <td>{{ parcour.dernierDiplome }}</td>
                <td>{{ parcour.dernierEmploi }}</td>
                <td>{{ parcour.dernierContrat }}</td>
                <td>{{ parcour.handicap }}</td>
                <td>{{ parcour.remarque }}</td>
                <td>{{ parcour.cv }}</td>
                <td>{{ parcour.nomReferent }}</td>
                <td>{{ parcour.mailReferent }}</td>
                <td>
                    <a href="{{ path('parcours_show', {'id': parcour.id}) }}">show</a>
                    <a href="{{ path('parcours_edit', {'id': parcour.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="12">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('parcours_new') }}">Create new</a>
{% endblock %}
