<form method="post" action="{{ path('prospect_delete', {'id': prospect.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ prospect.id) }}">
    <button class="btn btn-outline-info btn-lg " class="btn">supprimer</button>
</form>
